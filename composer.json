    "keywords": ["ssh", "key", "access"],
    "homepage": "http://github.com/Innmind/SshKeyProvider",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
